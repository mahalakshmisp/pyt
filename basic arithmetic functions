print("arithmetic functions")
x=input("enter the x value")
y=input("enter the y value")
r =int(x) + int(y)
z=r
print("addition program ")
print("the answer is",z)
c= int(x) - int(y)
print("subtraction program")
print("the answer is",c)
d= int(x) * int(y)
print("multiplication program")
print("the answer is",d)
e=int(x) / int(y)
print("division program")
print("the answer is",e)
f= int(x) % int(y)
print("modulation program")
print("the answer is",f)
